{
    "name": "codeages/phalcon-biz-api-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "基于 Phalcon、Biz Framework 的 API 项目脚手架。",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "codeages/phalcon-biz-library": "^1.0.11",
        "codeages/plumber2": "^0.4.6",
        "codeages/library": "^0.0.5",
        "datto/json-rpc-http": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6 || ^7 || ^8"
    },
    "config" : {
        "bin-dir" : "bin"
    }
}
